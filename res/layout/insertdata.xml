<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<TextView
			android:text="접종 기간 : "
			android:layout_width="120dp"
			android:textSize="17sp" 
			android:layout_height="wrap_content"/>
		<EditText
			android:id="@+id/edtMonth"
			android:textSize="17sp" 
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"/>
	</LinearLayout>
	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<TextView
			android:text="예방 접종 : "
			android:layout_width="120dp"
			android:textSize="17sp" 
			android:layout_height="wrap_content"/>
		<EditText
			android:id="@+id/edtVac"
			android:textSize="17sp" 
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"/>
	</LinearLayout>
	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<TextView
			android:text="보건소 접종 : "
			android:layout_width="120dp"
			android:textSize="17sp" 
			android:layout_height="wrap_content"/>
		<EditText
			android:id="@+id/edtBogun"
			android:textSize="17sp" 
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"/>
	</LinearLayout>
	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<TextView
			android:text="비고 : "
			android:layout_width="120dp"
			android:textSize="17sp" 
			android:layout_height="wrap_content"/>
		<EditText
			android:id="@+id/edtEtc"
			android:textSize="17sp" 
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"/>
	</LinearLayout>
</LinearLayout>
